    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^2.1.2",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2"
  }
